set y2range [ * : * ] noreverse writeback
set zrange [ * : * ] noreverse writeback
#set cblabel "Score" 
set rrange [ * : * ] noreverse writeback
set palette defined ( 0 'red', 1 'dark-blue', 2 'black')
NO_ANIMATION = 1
